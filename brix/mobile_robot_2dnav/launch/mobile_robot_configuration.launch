<?xml version="1.0"?>

<launch>

    <!-- start sensor
    <include file="$(find openni_launch)/launch/openni.launch">
        <arg name="camera" default="$(arg camera)"/>
    </include>-->

    <!-- laserscan node-->
    <node pkg="hokuyo_node" type="hokuyo_node" name="hokuyo_node">
	<param name="port" type="str" value="/dev/ttyACM0"/>
	<param name="frame_id" type="str" value="base_laser"/>
    </node>

    <node pkg="uvc_camera" type="uvc_camera_node" name="fcam">
	<param name = "device" type="string" value="/dev/video0"/>
	<remap from = "/image_raw" to = "/fcam/image_raw" />
    </node>
    <node pkg="uvc_camera" type="uvc_camera_node" name="lcam">
	<param name = "device" type="string" value="/dev/video1"/>
	<remap from = "/image_raw" to = "/lcam/image_raw" />
    </node>
    <node pkg="uvc_camera" type="uvc_camera_node" name="rcam">
	<param name = "device" type="string" value="/dev/video2"/>
	<remap from = "/image_raw" to = "/rcam/image_raw" />
    </node>
    <node pkg="beobot_control" type="beobot_motor" name="beobot_motor" />


  <!--
    <node pkg="robot_pose_ekf" type="robot_pose_ekf" name="pose_ekf" >
	  <param name="output_frame" value="odom_combined"/>
	  <param name="base_footprint_frame" value="base_link"/>
	  <param name="freq" value="30.0"/>
	  <param name="sensor_timeout" value="1.0"/>  
	  <param name="odom_used" value="true"/>
	  <param name="imu_used" value="true"/>
	  <param name="vo_used" value="false"/>

    </node>
  -->

   
    <node pkg="mobile_robot_setup_tf" type="tf_robot" name="tf_robot" />
    <node pkg="rosserial_python" type="serial_node.py" name="rosserial" output="screen" required="true">
        <param name="port" value="/dev/ttyACM1" />
        <param name="baud" value="57600" />
    </node>
    <remap from="cmd_vel" to="lidar_vel"/>  
</launch>
